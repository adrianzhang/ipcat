  <body>
      {{user_ip}}
          <pre>
          from flask import Flask, render_template, request
          # Initialize the Flask application
          app = Flask(__name__)
          # Default route, print user's IP ï¼Œhttp://www.361way.com
          @app.route('/')
          def index():
            ip = request.remote_addr
            return render_template('index.html', user_ip=ip)
          </pre>
  </body>
</html>
